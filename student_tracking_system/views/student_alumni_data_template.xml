<odoo>
	<data>
		<template id="student_alumni_data_template" name="Student Personal Info">
			<t t-call="website.layout">
				<body>
					<div class="container">
						<div class="container-xl">
							<div class="row pt-2">
								<div class="table-responsive table-wrapper">
									<div class="table-title">
										<div class="row">
											<div class="col-sm-6">
												<h2>
													<b>
														Alumni Data
													</b>
												</h2>
											</div>
											<div class="col-sm-6">
											</div>
										</div>
									</div>
									<table class="table table-striped table-hover">
										<thead>
											<tr>
												<th width="15%">
													Student ID
												</th>
												<th width="15%">
													Student Name
												</th>
												<th width="20%">
													Program Name
												</th>
												<th width="15%">
													Student Status
												</th>
												<th width="20%">
													Pass Out Date
												</th>
												<th width="15%">
													Convocation Number
												</th>
											</tr>
										</thead>

										<tbody>
											<tr>
												<td>
													<t t-if="student_alumni_info" t-esc="student_alumni_info['studenT_ID']" />
												</td>
												<td>
													<t t-if="student_alumni_info" t-esc="student_alumni_info['studenT_NAME']" />
												</td>
												<td>
													<t t-if="student_alumni_info" t-esc="student_alumni_info['prograM_NAME']" />
												</td>
												<td>
													<t t-if="student_alumni_info" t-esc="student_alumni_info['studentStatus']" />
												</td>
												<td>
													<t t-if="student_alumni_info" t-esc="student_alumni_info['pasS_OUT_DATE']" />
												</td>
												<td>
													<t t-if="student_alumni_info" t-esc="student_alumni_info['convocation_Number']" />
												</td>
												
											</tr>
										</tbody>
									</table>
									
								</div>
							</div>
						</div>
					</div>
				</body>
			</t>
		</template>
	</data>
</odoo>