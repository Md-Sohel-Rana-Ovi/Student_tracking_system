<odoo>
	<data>
		<template id="student_personal_info_template" name="Student Personal Info">
			<t t-call="website.layout">
				<body>
					<div class="container">
						<div class="container-xl">
							<div class="row pt-2">
								<div class="table-responsive table-wrapper">
									<div class="table-title">
										<div class="row">
											<div class="col-sm-6">
												<h2>
													<b>
														Personal Info
													</b>
												</h2>
											</div>
											<div class="col-sm-6">
												<!-- <a href="/dashboard?student_id=171-15-9367" class="btn btn-info">
													<i class="bi bi-arrow-left-circle-fill">
													</i>
													<span>
														Back to Dashboard dfdfdf
													</span>
												</a> -->
											</div>
										</div>
									</div>
									<table class="table table-striped table-hover">
										<thead>
											<tr>
												<th width="15%">
													Name
												</th>
												<th width="15%">
													Father name
												</th>
												<th width="15%">
													Permanent address
												</th>
												<th width="15%">
													Peresent address
												</th>
												<th width="15%">
													Email address
												</th>
												<th width="15%">
													Mobile number
												</th>
												<th width="10%">
													Nationality
												</th>
										
											</tr>
										</thead>
										
										<tbody>
										<tr>
										<td>
										<t t-if="student_info" t-esc="student_info['studentName']"/>
										</td>
										<td>
										<t t-if="student_info" t-esc="student_info['fatherName']"/>
										</td>
										<td>
										<t t-if="student_info" t-esc="student_info['presentAddress']"/>
										</td>
										<td>
										<t t-if="student_info" t-esc="student_info['permanentAddress']"/>
										</td>
										<td>
										<t t-if="student_info" t-esc="student_info['email']"/>
										</td>
										<td>
										<t t-if="student_info" t-esc="student_info['mobile']"/>
										</td>
										<td>
										<t t-if="student_info" t-esc="student_info['nationality']"/>
										</td>
										</tr>
										</tbody>
									</table>
								</div>
							</div>
						</div>
					</div>
				</body>
			</t>
		</template>
	</data>
</odoo>
